# URL base del API Gateway que expone los endpoints protegidos por Auth0
VITE_API_SERVER_URL=http://localhost:8085

# Credenciales de tu aplicación Auth0
VITE_AUTH0_DOMAIN=tu-dominio.us.auth0.com
VITE_AUTH0_CLIENT_ID=tuClientId123
VITE_AUTH0_AUDIENCE=https://uco-challenge-api

# URL a la que Auth0 redirige después de iniciar sesión.
# Si lo dejas vacío, la aplicación usará window.location.origin/callback.
VITE_AUTH0_CALLBACK_URL=http://localhost:5173/callback

# Scopes adicionales solicitados al iniciar sesión.
# Puedes ajustarlo a los scopes configurados en tu API de Auth0.
VITE_AUTH0_SCOPE=read:users read:settings
